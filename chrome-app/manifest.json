{

  "manifest_version": 2,
  "name": "Android FineTuner",
  "version": "0.1",
  "minimum_chrome_version": "33",
  "icons": {
    "128": "icon128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
	"permissions": [
	    "app.window", "storage"
	],

"sockets": { "tcp": { "connect" : "*:*" } } 
}
